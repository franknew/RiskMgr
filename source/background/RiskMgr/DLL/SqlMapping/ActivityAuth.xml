        [Name]=#ActivityAuth.Name#,
        </isNotNull>
        <isNotNull property="ActivityAuth.Creator">
        [Creator]=#ActivityAuth.Creator#,
        </isNotNull>
        <isNotNull property="ActivityAuth.CreateTime">
        [CreateTime]=#ActivityAuth.CreateTime#,
        </isNotNull>
