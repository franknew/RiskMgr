    </isNotNull>
  </delete>
   <select id="Query`WorkflowDefinition`LastUpdateTime" resultClass="DateTime">
        SELECT MAX(LastUpdateTime) AS LastUpdateTime FROM `WorkflowDefinition`
   </select>
  </statements>
</sqlMap>
